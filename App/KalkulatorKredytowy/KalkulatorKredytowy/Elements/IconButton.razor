<a href="@ButtonLink" class="btn btn-primary">
	@*<span class="oi @IconClass me-2" aria-hidden="true"/>*@
	<img src="@IconLink" style="max-width:16px; max-height:100%" class="m-1 d-inline-block">
	@ButtonText
</a>


@code {
	[Parameter]
	public string IconLink { get; set; }

	[Parameter]
	public string ButtonText { get; set; }

	[Parameter]
	public string ButtonLink {get;set;}
}
